{
  "scopeName": "source.tso",
  "patterns": [
    {
      "include": "#object"
    },
    {
      "include": "#ui"
    },
    {
      "include": "#script"
    }
  ],
  "repository": {
    "object": {
      "begin": "\\`\\`\\`object",
      "end": "\\`\\`\\`",
      "name": "embedded.yaml",
      "patterns": [
        {
          "include": "source.yaml"
        }
      ]
    },
    "ui": {
      "begin": "\\`\\`\\`ui",
      "end": "\\`\\`\\`",
      "name": "embedded.xml",
      "patterns": [
        {
          "include": "text.xml"
        }
      ]
    },
    "script": {
      "begin": "\\`\\`\\`script",
      "end": "\\`\\`\\`",
      "name": "embedded.lua",
      "patterns": [
        {
          "include": "source.lua"
        }
      ]
    }
  }
}
