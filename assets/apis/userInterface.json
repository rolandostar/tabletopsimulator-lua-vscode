{
  "elements": [
    {
      "name": "Text",
      "description": "Adds basic text. This tag supports Rich Text",
      "url": "https://api.tabletopsimulator.com/ui/basicelements/#text",
      "attributes": [
        {
          "name": "text",
          "type": "string",
          "description": "This can be used to determine the text that appears. It can also be modified externally by the script.",
          "default": "(none)"
        },
        {
          "name": "alignment",
          "type": "enum",
          "default": "MiddleCenter",
          "values": [
            "UpperLeft",
            "UpperCenter",
            "UpperRight",
            "MiddleLeft",
            "MiddleCenter",
            "MiddleRight",
            "LowerLeft",
            "LowerCenter",
            "LowerRight",
            "MiddleCenter"
          ]
        },
        {
          "name": "color",
          "type": "color",
          "default": "#323232"
        },
        {
          "name": "fontStyle",
          "type": "enum",
          "default": "Normal",
          "values": [
            "Normal",
            "Bold",
            "Italic",
            "BoldItalic"
          ]
        },
        {
          "name": "fontSize",
          "type": "float",
          "default": "14"
        },
        {
          "name": "resizeTextForBestFit",
          "type": "bool",
          "description": "Resize text to fit?",
          "default": "false"
        },
        {
          "name": "resizeTextMinSize",
          "type": "float",
          "description": "Minimum font size",
          "default": "10"
        },
        {
          "name": "resizeTextMaxSize",
          "type": "float",
          "description": "Maximum font size",
          "default": "40"
        },
        {
          "name": "horizontalOverflow",
          "type": "enum",
          "default": "Overflow",
          "values": [
            "Wrap",
            "Overflow"
          ]
        },
        {
          "name": "verticalOverflow",
          "type": "enum",
          "default": "Truncate",
          "values": [
            "Truncate",
            "Overflow",
            "Truncate"
          ]
        }
      ]
    },
    {
      "name": "Image",
      "description": "Adds an image.",
      "url": "https://api.tabletopsimulator.com/ui/basicelements/#image",
      "attributes": [
        {
          "name": "image",
          "type": "string",
          "description": "The name of the file in the asset manager (upper right corner of the scripting window in-game).",
          "default": "(none)"
        },
        {
          "name": "color",
          "type": "color",
          "default": "#FFFFFF"
        },
        {
          "name": "type",
          "type": "enum",
          "description": "Image Type",
          "default": "Simple",
          "values": [
            "Simple",
            "Sliced",
            "Filled",
            "Tiled"
          ]
        },
        {
          "name": "raycastTarget",
          "type": "bool",
          "description": "Should this image block clicks from passing through it?",
